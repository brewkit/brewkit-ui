{
    "name": "@brewkit/components",
    "version": "4.0.0",
    "description": "Brewkit components",
    "main": "index.js",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/brewkit/components"
    },
    "author": "lawren",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/brewkit/components/issues"
    },
    "homepage": "https://brewkit.github.io",
    "sideEffects": false,
    "scripts": {
        "test:generate-output": "jest --watchAll --json --outputFile=.build/jest/test-results.json || true",
        "test": "jest --watchAll --json --outputFile=.build/jest/test-results.json",
        "commit": "npx git-cz",
        "start": "npm run storybook",
        "storybook": "start-storybook -p 9001 -c .build/storybook",
        "build": "node .build/copy-package-files.js && ttsc --project ./.build/tsconfig.json",
        "build-storybook": "build-storybook -c .build/storybook -o .out",
        "deploy-storybook": "storybook-to-ghpages",
        "release": ".build/release.sh"
    },
    "jest": {
        "transform": {
            ".(ts|tsx)": "ts-jest"
        },
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/lib/"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "json"
        ],
        "moduleNameMapper": {
            "^@components/(.+)$": "<rootDir>/src/components/$1",
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|md)$": "<rootDir>/.build/jest/mocks/fileMock.js",
            "^.+\\.(css|less|scss)$": "identity-obj-proxy"
        },
        "setupFilesAfterEnv": [
            "<rootDir>/.build/jest/setupTests.js"
        ]
    },
    "devDependencies": {
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.56",
        "@storybook/addon-actions": "^6.0.22",
        "@storybook/addon-backgrounds": "^6.0.22",
        "@storybook/addon-docs": "^6.0.22",
        "@storybook/addon-jest": "^6.0.22",
        "@storybook/addon-knobs": "^6.0.22",
        "@storybook/addon-storyshots": "^6.0.22",
        "@storybook/addon-viewport": "^6.0.22",
        "@storybook/addons": "^6.0.22",
        "@storybook/preset-typescript": "^3.0.0",
        "@storybook/react": "^6.0.22",
        "@storybook/storybook-deployer": "^2.8.6",
        "@storybook/theming": "^6.0.22",
        "@types/enzyme": "^3.10.6",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/jest": "^26.0.13",
        "@types/lodash": "^4.14.161",
        "@types/node": "^14.6.4",
        "@types/react": "^16.9.49",
        "@types/react-dom": "^16.9.8",
        "@types/react-router-dom": "^5.1.5",
        "@types/react-test-renderer": "^16.9.3",
        "@types/yup": "^0.29.6",
        "@typescript-eslint/eslint-plugin": "^4.2.0",
        "@typescript-eslint/parser": "^4.2.0",
        "@zerollup/ts-transform-paths": "^1.7.18",
        "babel-loader": "^8.1.0",
        "cz-conventional-changelog": "^3.3.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.5",
        "eslint": "^7.10.0",
        "eslint-plugin-react": "^7.21.2",
        "fork-ts-checker-webpack-plugin": "^5.2.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.4.2",
        "react": "^16.13.1",
        "react-docgen-typescript-loader": "^3.7.2",
        "react-dom": "^16.13.1",
        "react-hot-loader": "^4.13.0",
        "react-router-dom": "^5.2.0",
        "react-test-renderer": "^16.13.1",
        "ts-jest": "^26.4.0",
        "ts-loader": "^8.0.4",
        "ttypescript": "^1.5.12",
        "typescript": "^4.0.3"
    },
    "peerDependencies": {
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.2.0",
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.56"
    },
    "dependencies": {
        "@popperjs/core": "^2.5.2",
        "clsx": "^1.1.1",
        "framer-motion": "^2.7.5",
        "lodash": "^4.17.20",
        "react-hook-form": "^6.8.6"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "release": {
        "branches": [
            "master",
            "next"
        ]
    }
}
