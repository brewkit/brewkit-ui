{
    "name": "@brewkit/components",
    "version": "4.7.1",
    "description": "Brewkit components",
    "main": "index.js",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/brewkit/components"
    },
    "author": "lawren",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/brewkit/components/issues"
    },
    "homepage": "https://brewkit.github.io",
    "sideEffects": false,
    "scripts": {
        "install-bkg": "cd .build/bkg-cli && npm link",
        "install-test-runner": "cd .build/jest && npm link",
        "commit": "npx git-cz",
        "start": "npm run storybook",
        "storybook": "start-storybook -p 9001 -c .build/storybook",
        "build": "node .build/scripts/copy-package-files.js && npx ttsc --project ./.build/tsconfig.json",
        "build-themes": "npx ttsc --project ./themes/tsconfig.json",
        "build-storybook": "build-storybook -c .build/storybook -o .out",
        "postversion": "node .build/scripts/copy-package-files.js && node .build/scripts/sync-versions.js",
        "deploy-storybook": "storybook-to-ghpages"
    },
    "devDependencies": {
        "@jest/types": "^27.4.2",
        "@material-ui/core": "^4.12.3",
        "@material-ui/icons": "^4.11.2",
        "@material-ui/lab": "^4.0.0-alpha.60",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^8.0.2",
        "@semantic-release/npm": "^8.0.3",
        "@storybook/addon-actions": "^6.4.9",
        "@storybook/addon-backgrounds": "^6.4.9",
        "@storybook/addon-docs": "^6.4.9",
        "@storybook/addon-essentials": "^6.4.9",
        "@storybook/addon-knobs": "^6.4.0",
        "@storybook/addon-storyshots": "^6.4.9",
        "@storybook/addon-viewport": "^6.4.9",
        "@storybook/addons": "^6.4.9",
        "@storybook/preset-typescript": "^3.0.0",
        "@storybook/react": "^6.4.9",
        "@storybook/storybook-deployer": "^2.8.10",
        "@storybook/theming": "^6.4.9",
        "@testing-library/dom": "^8.11.1",
        "@testing-library/jest-dom": "^5.16.1",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^13.5.0",
        "@types/color": "^3.0.2",
        "@types/enzyme": "^3.10.10",
        "@types/lodash": "^4.14.178",
        "@types/node": "^14.18.2",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@types/react-router-dom": "^5.3.2",
        "@types/react-test-renderer": "^17.0.1",
        "@types/yup": "^0.29.13",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
        "@zerollup/ts-transform-paths": "^1.7.18",
        "babel-loader": "^8.2.3",
        "enzyme": "^3.11.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.27.1",
        "eslint-plugin-storybook": "^0.5.5",
        "execa": "^4.1.0",
        "jest": "^27.4.5",
        "listr2": "^3.13.5",
        "majestic": "^1.8.1",
        "prettier": "^2.5.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-hot-loader": "^4.13.0",
        "react-router-dom": "^5.3.0",
        "react-test-renderer": "^17.0.2",
        "semantic-release": "^18.0.1",
        "storybook-dark-mode": "^1.0.8",
        "ts-jest": "^27.1.2",
        "ts-loader": "^8.0.18",
        "ts-node": "^10.4.0",
        "ttypescript": "^1.5.13",
        "typescript": "^4.5.4",
        "yargs": "^17.3.0"
    },
    "peerDependencies": {
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.56",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-router-dom": "^5.2.0"
    },
    "dependencies": {
        "@popperjs/core": "^2.11.0",
        "clsx": "^1.1.1",
        "color": "^3.2.1",
        "framer-motion": "^3.10.3",
        "lodash": "^4.17.21",
        "react-hook-form": "^7.22.3"
    },
    "majestic": {
        "jestScriptPath": "node_modules/jest/bin/jest",
        "args": [
            "--config=jest.config.js"
        ],
        "env": {
            "CI": "true"
        }
    },
    "release": {
        "branches": [
            "main"
        ],
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/github",
            "@semantic-release/npm",
            [
                "@semantic-release/git",
                {
                    "assets": [
                        "package.json",
                        "themes/package.json"
                    ],
                    "message": "ðŸ¤– chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
                }
            ]
        ]
    }
}
